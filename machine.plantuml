class XXXAddress {	+ port	+ host	-- magritte description --	+ descriptionHost()	+ descriptionPort()	-- accessing --	{static} + xmlElementName()}note right of XXXAddress::descriptionHost	descriptionHost	<magritteDescription>	^ MAStringDescription new		accessor: #host;		label: 'host';		priority: 1;		beXmlElement;		yourselfend notenote right of XXXAddress::descriptionPort	descriptionPort	<magritteDescription>	^ MANumberDescription new		accessor: #port;		label: 'port';		priority: 2;		beXmlElement;		yourselfend notenote right of XXXAddress::xmlElementName	xmlElementName	^ 'address'end noteclass XXXMachine {	+ version	+ nodes	+ anyOnline	+ id	-- magritte description --	+ descriptionAnyOnline()	+ descriptionId()	+ descriptionIsOnline()	+ descriptionNodes()	+ descriptionVersion()	-- accessing --	{static} + xmlElementName()}note right of XXXMachine::descriptionAnyOnline	descriptionAnyOnline	<magritteDescription>	^ MABooleanDescription new		accessor: #anyOnline;		label: 'anyOnline';		priority: 3;		beXmlElement;		yourselfend notenote right of XXXMachine::descriptionId	descriptionId	<magritteDescription>	^ MANumberDescription new		accessor: #id;		label: 'id';		priority: 1;		beXmlAttribute;		yourselfend notenote right of XXXMachine::descriptionIsOnline	descriptionIsOnline	<magritteDescription>	^ MABooleanDescription new		accessor: #isOnline;		label: 'isOnline';		priority: 3;		beXmlElement;		yourselfend notenote right of XXXMachine::descriptionNodes	descriptionNodes	<magritteDescription>	^ MAToManyRelationDescription new		accessor: #nodes;		label: 'nodes';		priority: 4;		classes: { XXXNode };		beXmlElement;		yourselfend notenote right of XXXMachine::descriptionVersion	descriptionVersion	<magritteDescription>	^ MAStringDescription new		accessor: #version;		label: 'version';		priority: 2;		beXmlChildAttribute;		xmlAttributeName: 'value';		yourselfend notenote right of XXXMachine::xmlElementName	xmlElementName	^ 'machine'end noteclass XXXNode {	+ isOnline	+ id	+ address	-- magritte description --	+ descriptionAddress()	+ descriptionId()	+ descriptionIsOnline()	-- accessing --	{static} + xmlElementName()}note right of XXXNode::descriptionAddress	descriptionAddress	<magritteDescription>	^ MAToOneRelationDescription new		accessor: #address;		label: 'address';		priority: 3;		classes: { XXXAddress };		beXmlInlineElement;		yourselfend notenote right of XXXNode::descriptionId	descriptionId	<magritteDescription>	^ MAStringDescription new		accessor: #id;		label: 'id';		priority: 1;		beXmlAttribute;		yourselfend notenote right of XXXNode::descriptionIsOnline	descriptionIsOnline	<magritteDescription>	^ MABooleanDescription new		accessor: #isOnline;		label: 'isOnline';		priority: 2;		beXmlElement;		yourselfend notenote right of XXXNode::xmlElementName	xmlElementName	^ 'node'end note	XXXMachine --> "nodes *" XXXNode	XXXNode --> "address 1" XXXAddressclass ":XXXMachine" {	version = '1.2.7'	anyOnline = false	id = 12}":XXXMachine" .> XXXMachine : <<instanceOf>>class ":XXXNode" {	isOnline = false	id = 'node-1'}":XXXNode" .> XXXNode : <<instanceOf>>class ":XXXAddress" {	port = 80	host = '127.0.0.1'}":XXXAddress" .> XXXAddress : <<instanceOf>>